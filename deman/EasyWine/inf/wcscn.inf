;; .INF script for the Chinese-specific configuration
;;
;; This should be run through setupapi:
;;   rundll32 setupapi.dll,InstallHinfSection DefaultInstall 128 wcscn.inf

[version]
signature="$CHICAGO$"

[DefaultInstall]
UpdateInis=WinIni

[WinIni]
win.ini, Desktop,,"MenuFontSize=12"
win.ini, Desktop,,"MessageFontSize=12"
win.ini, Desktop,,"StatusFontSize=12"
win.ini, Desktop,,"IconTitleSize=12"

